<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
   <interface name="org.kde.KPackageKitSmartIcon">
       <method name="WatchTransaction" >
           <!-- IN: transaction id -->
           <arg direction="in" type="s" name="tid" />
       </method>
       <method name="RefreshCache" >
       </method>
       <method name="Update" >
       </method>
       <method name="RefreshAndUpdate" >
       </method>
       <method name="DebconfDialog" >
           <!-- IN: path to the DebconfDialog socket -->
           <arg direction="in" type="s" name="socket_path" />
           <!-- IN: xid of the Debconf Dialog parent -->
           <arg direction="in" type="u" name="xid_parent">
       </method>
   </interface>
</node>
